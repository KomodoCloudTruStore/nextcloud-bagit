<?xml version="1.0"?>
<info xmlns:xsi= "http://www.w3.org/2001/XMLSchema-instance"
      xsi:noNamespaceSchemaLocation="https://apps.nextcloud.com/schema/apps/info.xsd">
    <id>bagit</id>
    <name>BagIt</name>
    <namespace>BagIt</namespace>
    <description>Allows users to create BagIt containers from items in the Files App.</description>
    <licence>agpl</licence>
    <author homepage="https://github.com/KomodoCloudTruStore/nextcloud-bagit/graphs/contributors">KomodoCloudTruStore</author>
    <version>0.1.7</version>
    <category>tool</category>
    <bugs>https://github.com/KomodoCloudTruStore/nextcloud-bagit/issues</bugs>
    <dependencies>
        <owncloud min-version="9.2"/>
        <nextcloud min-version="11" max-version="13"/>
    </dependencies>
    <activity>
        <providers>
            <provider>OCA\BagIt\Activity\Provider</provider>
        </providers>
        <settings>
            <setting>OCA\BagIt\Activity\Setting</setting>
        </settings>
        <filters>
            <filter>OCA\BagIt\Activity\Filter</filter>
        </filters>
    </activity>
</info>

